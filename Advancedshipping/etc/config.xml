<config>

	<modules>
		<Plumtree_Advancedshipping>
			<version>0.1.0</version>
		</Plumtree_Advancedshipping>
	</modules>

	<frontend>
		<routers>
			<advancedshipping>
				<args>
					<modules>
						<Plumtree_Advancedshipping before="Mage_Shipping">Plumtree_Advancedshipping</Plumtree_Advancedshipping>
					</modules>
				</args>
			</advancedshipping>
		</routers>
	</frontend>
	<global>
		<helpers>
            <advancedshipping>
                <class>Plumtree_Advancedshippings_Helper</class>
            </advancedshipping>
        </helpers>
	</global>
	
